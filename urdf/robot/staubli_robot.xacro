</xacro:macro>

<xacro:macro name="box_inertial" params="m x y z rpy">
  <inertial>
    <origin xyz="0 ${y/2} 0" rpy="${rpy}" />
    <mass value="${m}"/>
    <inertia ixx="${1/12 * m * (y**2 + z**2)}" iyy="${1/12 * m * (x**2 + z**2)}" izz="${1/12 * m * (x**2 + y**2)}" ixy="0" ixz="0" iyz="0"/>
  </inertial>
</xacro:macro>

